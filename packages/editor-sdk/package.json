    "codemirror": "^5.63.3",
    "formik": "^2.2.9",
    "framer-motion": "^3.10.6",
    "immer": "^9.0.6",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "mobx": "^6.3.8",
    "mobx-react-lite": "^3.2.2",
    "rc-select": "^14.1.5",
    "react-color": "^2.19.3"
